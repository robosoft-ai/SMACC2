<!-- Use gazebo_ros_joint_state_publisher instead of publishWheelJointState -->
      <plugin name="joint_states" filename="libgazebo_ros_joint_state_publisher.so">
        <joint_name>mp_400_fixed_wheel_left_joint</joint_name>
        <joint_name>mp_400_fixed_wheel_right_joint</joint_name>
      </plugin>


      <plugin name='diff_drive' filename='libgazebo_ros_diff_drive.so'>
        <ros>        </ros>
        <update_rate>100</update_rate>
        <left_joint>mp_400_fixed_wheel_left_joint</left_joint>
        <right_joint>mp_400_fixed_wheel_right_joint</right_joint>
        <wheel_separation>0.529</wheel_separation>
        <wheel_diameter>0.15</wheel_diameter>
        <publish_odom>true</publish_odom>
        <publish_odom_tf>true</publish_odom_tf>
        <publish_wheel_tf>true</publish_wheel_tf>
        <command_topic>cmd_vel</command_topic>

        <odometry_topic>odom</odometry_topic>
        <odometry_frame>odom</odometry_frame>
        <robot_base_frame>base_footprint</robot_base_frame>

        <!-- wheelTorque and wheelAcceleration now have max_ prefix -->
        <max_wheel_torque>1000</max_wheel_torque>
        <max_acceleration>20.0</max_acceleration>

      </plugin>
</gazebo>

